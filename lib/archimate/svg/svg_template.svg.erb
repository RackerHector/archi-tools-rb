      </symbol>
      <symbol id="archimate-location-badge" class="archimate-badge"  viewBox="0 0 20 15">
        <path d="m6 7.5 a 5 5, 0, 1, 1, 8 0 l -4 7 z" style="fill:inherit;stroke:inherit"/>
      </symbol>
      <symbol id="archimate-interaction-badge" class="archimate-badge"  viewBox="0 0 20 15" preserveAspectRatio="xMaxYMin meet">
        <path d="M 8 14 a 5 6 0 0 1 0 -13 z" style="fill:inherit;stroke:inherit"/>
        <path d="M11 14 a 5 6 0 0 0 0 -13 z" style="fill:inherit;stroke:inherit"/>
      </symbol>
      <symbol id="archimate-communication-badge" class="archimate-badge"  viewBox="0 0 20 15">
