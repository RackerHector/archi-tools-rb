        <path d="m15 12 h -10 a 4.5 4.5 0 0 1 0 -9 h 10 a 4.5 4.5 0 0 1 0 9 a 4.5 4.5 0 0 1 0 -9" style="fill:inherit;stroke:inherit"/>
      </symbol>
      <symbol id="archimate-collaboration-badge" class="archimate-badge"  viewBox="0 0 20 15" preserveAspectRatio="xMaxYMin meet">
